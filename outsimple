a=float(input("intuduza um numero para a "))
print(("intuduzui para a:", a))
b=float(input("intuduza um numero para b "))
print(("intuduzui para a:", b))
print(float(a+b))
print(float(a-b))
print(float(a*b))
print(float(a/b))
print("\nThat's all, folks!")


